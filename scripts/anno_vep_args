parser.add_argument('--input_vcf', required=False, help='Input VCF file (must be bgzipped)')
parser.add_argument('--config', default='', help='Config file path')
parser.add_argument('--assembly', help='Assembly version (hg19/hg38)')
parser.add_argument('--ref_genome', help='Reference genome FASTA')
parser.add_argument('--vep_cache_dir', help='VEP cache directory')
parser.add_argument('--vep_plugins_dir', help='VEP plugins directory')
parser.add_argument('--vep_plugins_cachedir', help='VEP plugins cache directory')
parser.add_argument('--threads', type=int, default=4, help='Number of threads')
parser.add_argument('--utr_annotator_file', help='UTRAnnotator plugin cache file')
parser.add_argument('--loeuf_prescore', help='LOEUF plugin cache file')
parser.add_argument('--alphamissense_prescore', help='AlphaMissense plugin cache file')
parser.add_argument('--spliceai_snv_prescore', help='SpliceAI SNV plugin cache file')
parser.add_argument('--spliceai_indel_prescore', help='SpliceAI indel plugin cache file')
parser.add_argument('--primateai_prescore', help='PrimateAI plugin cache file')
parser.add_argument('--conversation_file', help='Conservation plugin cache file')
